#include "stdafx.h"
#include "stdio.h"
#include "conio.h"


int main(void)
{
	int num1;
	int num2;
	int multi;
	int resultado;

	printf("Digite valor 1 ");
	scanf ("%d", &num1);
	printf("Digite valor 2 ");
    scanf ("%d", &num2);
    multi = num1*num2;
	resultado = multi* multi;
	printf ("El cuadrado de la multiplicacion es %d ", resultado);
	getch();
	return 0;
}
